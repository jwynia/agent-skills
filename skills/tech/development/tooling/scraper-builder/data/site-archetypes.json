  },
  "job_board": {
    "description": "Job listing sites with postings, search, and application tracking",
    "typical_pages": [
      "JobListingPage",
      "JobDetailPage",
      "CompanyPage",
      "SearchResultsPage"
    ],
    "typical_components": [
      "Pagination",
      "JobCard",
      "FilterSidebar",
      "SalaryRange",
      "CompanyInfo"
