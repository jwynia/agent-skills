    ],
    "typical_components": [
      "Pagination",
      "PropertyCard",
      "MapView",
      "ImageGallery",
      "FilterSidebar",
      "PriceRange"
    ],
    "typical_data": {
      "property": ["title", "price", "currency", "address", "bedrooms", "bathrooms", "sqft", "type", "status", "description", "image_urls", "agent", "listed_date"],
      "agent": ["name", "phone", "email", "agency"]
